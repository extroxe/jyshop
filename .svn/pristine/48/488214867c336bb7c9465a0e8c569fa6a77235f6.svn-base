header .titlebar h1 {
    margin-right: 40px;
}

.tabbar.tabbar-line.animate li {
    background-color: #FFFFFF;
}

.tabbar-line .tab.active {
    color: #F6BF00;
}

.tabbar-line .tab.active:after {
    background-color: #F6BF00;
    width: 30%;
}

.info-screen {
    background-color: #FFFFFF;
    padding: 15px 0;
}

.info-screen > div {
    padding: 0 29px;
    display: inline-block;
}

.info-screen > div.active {
    color: #F6BF00;
}

.info-screen .triangle {
    display: inline-block;
    margin-top: 2px;
}

.info-screen .triangle > span {
    display: block;
    top: 3px;
}

.info-screen .triangle > span:nth-child(1){
    margin-bottom: 2px;
}

.info-screen .triangle .triangle-up.active {
    border-bottom: 6px solid #F6BF00;
}

.info-screen .triangle .triangle-down.active {
    border-top: 6px solid #F6BF00;
}

.triangle-up {
    position: relative;
    width: 0;
    height: 0;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-bottom: 6px solid #DADADA;
}

.triangle-down {
    position: relative;
    width: 0;
    height: 0;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 6px solid #DADADA;
}

.card {
    margin: 0;
    background-color: #FFFFFF;
}

.card > div,
.card .info-box > div {
    padding: 10px;
    color: #666666;
}

.card .info-box .info-title {
    color: #333333;
    line-height: 16px;
    overflow: hidden;
    text-overflow:ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    padding-bottom: 0;
    padding-top: 0;
}

.card .info-box .info-group {
    position: absolute;
    bottom: 0;
}

.card .info-box .info-price {
    color: #F6BF00;
    font-size: 18px;
    padding-bottom: 17px;
}

.card .info-box .info-bottom {
    font-size: 12px;
    color: #666666;
}
.card .info-box .info-button a{
    width: 60px;
    height: 30px;
    position: absolute;
    right: 20px;
    bottom: 20px;
    border-color: #F6BF00;
    color: #969696;
    border-radius: 5px;
}
.card img {
    width: 100px;
    height: 100px;
}
hr {
    color: #969696;
}
.info-title {
    overflow:hidden;
    text-overflow:ellipsis;
    -o-text-overflow:ellipsis;
    white-space:nowrap;
    width:100%;
}


