header .titlebar h1 {
    /*margin-right: 40px;*/
}

.address-info {
    margin: 0 15px;
    border: solid 2px #117d94;
    border-radius: 5px;
    background-color: #FFFFFF;
}

.page-link-address {
    position: absolute;
    width: 384px;
    height: 144px;
    left: 0;
    top: 65px;
    margin-left: 15px;
}

.address-info p:nth-child(1) {
    padding: 15px 20px;
}

.address-info p:nth-child(3) {
    padding: 20px 20px;
}

.address-info .address-user-info > span:nth-child(1) {
    color: #666666;
}

.address-info .address-user-info > span:nth-child(3) {
    display: block;
    float: right;
    padding-top: 1px;
}

.address-info .address-place {
    display: flex;
}

.address-info .address-place > span:nth-child(1){
    margin-right: 10px;
    margin-top: -8px;
}

.address-info .address-place > span:nth-child(2){
    width: 75%;
}

.address-info .address-place > span:nth-child(3){
    position: absolute;
    right: 25px;
    margin-top: -15px;
}

.address-info .address-place > span:nth-child(3) > i {
    color: #666666;
}

.address-info .address-place > span > img {
    height: 40px;
}

.card {
    margin: 15px 0 8px 0;
    background-color: #FFFFFF;
}

.card > div,
.card .info-box > div {
    padding: 13px 20px;
    color: #666666;
}

.card > div:nth-child(2) {
    display: flex
}

.card .info-box .info-title {
    color: #333333;
    font-size: 16px;
    line-height: 16px;
    overflow: hidden;
    text-overflow:ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    padding-bottom: 0;
    margin-bottom: 18px;
}

.card .info-box .info-price {
    color: #F6BF00;
    font-size: 20px;
}

.card img {
    width: 120px;
    height: 120px;
}

.card .info-box {
    display: inline-block;
}

.card .item-num {
    position: absolute;
    right: 25px;
    bottom: 48px;
}

.card .inputbox .input-text{
    padding: 8px 12px;
    text-align: right;
}

.card .inputbox.message {
    padding: 10px 20px;
}

.card .item-span-right {
    position: absolute;
    right: 20px;
    top: 18px;
}

.page-link-discount {
    width: 20px;
    height: 20px;
    position: absolute;
    z-index: 100;
}

.card .item-info {
    display: list-item;
}

.card .item-info span:nth-child(2) {
    color: #F6BF00;
}

.card .item-info span:nth-child(3) {
    color: #F6BF00;
    font-size: 12px;
}

.confirm-order-footer {
    width: 100%;
    height: 55px;
    display: inline-flex;
    position: fixed;
    bottom: 0;
    background-color: #FFFFFF;
    text-align: center;
}

.confirm-order-footer div {
    padding: 16px 0 0 0;
    display: inline-block;
    color: #f9f9f9;
}

.confirm-order-footer .item-total {
    width: 70%;
    color: #333333;
    font-size: 16px;
    text-align: right;
    padding-top: 6px;
    padding-right: 15px;
    background-color: #FFFFFF;
}

.confirm-order-footer .item-total > div {
    padding: 0;
    color: #333333;
    display: block;
}

.confirm-order-footer .item-pay {
    width: 30%;
    background-color: #117D94;
    font-size: 18px;
}

.confirm-order-footer label {
    margin: 0;
    height: auto;
    line-height: initial;
}


/*地址-<<<<<<*/
.address-item {
    margin-top: 20px;
}

.address-item:nth-child(1) {
    margin-top: 0;
}

.address-item .item {
    font-size: 16px;
    padding: 15px;
    background-color: #FFFFFF;
}

.address-item .item-title > span:nth-child(2) {
    line-height: 25px;
    float: right;
}

.address-item .item-info {
    color: #666666;
    font-size: 14px;
    margin-top: 25px;
}

.address-item input[type=radio]:checked {
    color: #117D94;
}

.address-item .item-operation > span:nth-child(2) {
    margin-left: 10px;
}

.address-item .item-operation > span:nth-child(3),
.address-item .item-operation > span:nth-child(4) {
    float: right;
}

.address-item .item-operation > span:nth-child(4) {
    margin-right: 5px;
}

.address-item .item-operation img {
    width: 25px;
    margin: 0 10px;
}

.address-item .item-operation .icon {
    vertical-align: sub;
}

/*地址->>>>>>*/

section {
    height: 105%;
}
@media only screen and (max-width: 375px){
    .page-link-address {
        width: 345px;
        height: 144px;
    }

    section {
        height: 112%;
    }
}

@media only screen and (max-width: 320px){
    article {
        font-size: 12px;
    }

    .page-link-address {
        width: 290px;
        height: 132px;
    }

    .card > div,
    .card .info-box > div {
        padding: 15px 20px;
    }

    .card img {
        width: 100px;
        height: 100px;
    }

    .card .info-box .info-price {
        font-size: 20px;
        padding: 10px 20px;
    }

    .card .info-box .info-title {
        margin-bottom: 15px;
    }

    .card .inputbox.message {
        padding: 5px 20px;
    }

    .card .item-num {
        bottom: 22px;
    }

    .address-item .item {
        font-size: 14px;
    }

    .address-item .item-info {
        font-size: 13px;
    }

    .address-item .item-operation img {
        width: 20px;
    }

    .address-item .item-operation .icon {
        font-size: 18px;
        width: 18px;
        height: 18px;
    }

    section {
        height: 115%;
    }
}

/*优惠券>>>>>>>*/
#page_discount article .address-item {
    display: flex;
    height: 140px;
    margin-left: 5%;
    margin-top: 15px;
    background-repeat: no-repeat;
    background-size: 95%;
    background-image: url('../img/discount-coupon.png');
}

#page_discount article .address-item.unused {
    background-image: url('../img/discount-coupon-ed.png');
}

.address-item .discount-privilege {
    color: #FFFFFF;
    margin: 5px 0 0 20px;
    font-size: 26px;
    font-weight: 700;
    width: 140px;
}

.address-item .discount-privilege span {
    font-size: 75px;
    font-stretch: condensed;
    vertical-align: baseline;
}

.address-item .discount-condition {
    color: #FFFFFF;
    margin-top: 28px;
    font-size: 15px;
    text-align: center;
}

.address-item .discount-condition p:nth-child(2) {
    font-size: 22px;
    font-weight: 700;
}

.address-item .discount-btn {
    margin: 35px 0 0 35px;
}

.address-item .discount-btn button {
    height: 32px;
    color: #FFFFFF;
    font-size: 16px;
    padding: 5px 10px;
    border-radius: 5px;
    border: 1px solid #FFFFFF;
    background-color: transparent;
}

.address-item .discount-bottom {
    position: absolute;
    margin-top: 102px;
    margin-left: 20px;
    color: #999999;
    left: 20px;
}

@media only screen and (max-width: 375px) {
    .address-item .discount-privilege {
        width: 130px;
    }

    .address-item .discount-privilege span {
        font-size: 60px;
    }

    .address-item .discount-condition {
        font-size: 14px;
        margin-top: 25px;
    }

    .address-item .discount-condition p:nth-child(2) {
        font-size: 20px;
    }

    .address-item .discount-btn {
        margin: 35px 0 0 0;
    }

    .address-item .discount-btn button {
        height: 30px;
        font-size: 15px;
    }

    .address-item .discount-bottom {
        margin-top: 92px;
        font-size: 14px;
    }
}

@media only screen and (max-width: 320px) {
    .address-item .discount-privilege {
        margin: 7px 0 0 20px;
        font-size: 20px;
        width: 100px;
    }

    .address-item .discount-privilege span {
        font-size: 55px;
    }

    .address-item .discount-condition {
        font-size: 13px;
        margin-top: 20px;
    }

    .address-item .discount-condition p:nth-child(2) {
        font-size: 18px;
    }

    .address-item .discount-btn {
        margin: 30px 0 0 25px;
    }

    .address-item .discount-btn button {
        height: 25px;
        font-size: 13px;
    }

    .address-item .discount-bottom {
        margin-top: 77px;
        font-size: 14px;
    }
}