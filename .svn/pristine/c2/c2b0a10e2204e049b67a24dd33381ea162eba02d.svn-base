app.controller("categoryCtrl",["$scope","_jiyin","dataToURL",function(t,a,e){t.categoryList={},t.inputPage=1,t.getData=function(){a.dataGet("admin/category_admin/paginate/"+t.inputPage+"/10").then(function(a){t.categoryList=a.data,t.totalPage=a.total_page})},t.getData(),t.addList=function(){t.title="增加数据",a.modal({tempUrl:"/source/admin/tpl/modal/modal-category.html",tempCtrl:"modalCategoryCtrl",ok:t.add,size:"lg",params:{title:t.title,infoList:{},ael:"add"}})},t.add=function(i){a.dataPost("admin/category_admin/add",e(i)).then(function(e){1==e.success?(a.msg("s",e.msg),t.getData()):a.msg("e",e.msg)})},t.editList=function(e){t.title="编辑数据",a.modal({tempUrl:"/source/admin/tpl/modal/modal-category.html",tempCtrl:"modalCategoryCtrl",ok:t.edit,size:"lg",params:{title:t.title,infoList:e,ael:"edit"}})},t.edit=function(i){delete i.father_name,a.dataPost("admin/category_admin/update",e(i)).then(function(e){1==e.success?(a.msg("s",e.msg),t.getData()):a.msg("e",e.msg)})},t.deleteData=function(i){confirm("确认删除这条数据吗?")&&a.dataPost("admin/category_admin/delete",e({id:i.id})).then(function(){a.msg("s","删除成功"),t.getData()})},t.nextPage=function(){t.inputPage<t.totalPage?(t.inputPage++,t.getData()):a.msg("e","当前是最后一页")},t.previousPage=function(){t.inputPage>1?(t.inputPage--,t.getData()):a.msg("e","当前是第一页")},t.firstPage=function(){t.inputPage=1,t.getData()},t.lastPage=function(){t.inputPage=t.totalPage,t.getData()},t.selectPage=function(a){t.inputPage=a,t.getData()}}]);