app.controller("NoteCtrl",["$scope","$http",function(e,t){t.get("js/app/note/notes.json").then(function(t){e.notes=t.data.notes,e.note=e.notes[0],e.notes[0].selected=!0}),e.colors=["primary","info","success","warning","danger","dark"],e.createNote=function(){var t={content:"New note",color:e.colors[Math.floor(3*Math.random())],date:Date.now()};e.notes.push(t),e.selectNote(t)},e.deleteNote=function(t){e.notes.splice(e.notes.indexOf(t),1),t.selected&&(e.note=e.notes[0],e.notes.length&&(e.notes[0].selected=!0))},e.selectNote=function(t){angular.forEach(e.notes,function(e){e.selected=!1}),e.note=t,e.note.selected=!0}}]);