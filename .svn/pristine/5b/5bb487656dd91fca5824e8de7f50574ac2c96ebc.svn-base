app.controller("sweepCtrl",["$scope","_jiyin","dataToURL",function(t,e,n){t.content="",t.getSetting=function(){e.dataPost("admin/system_setting_admin/get_sweepstakes_rules").then(function(e){t.content=e.data.value})},t.getSetting(),t.save=function(){return t.content?void e.dataPost("admin/system_setting_admin/sweepstakes_rules",n({content:t.content})).then(function(n){1==n.success&&(e.msg("s","设置成功"),t.getSetting())}):void e.msg("e","请先设置积分抽奖规则")}}]);