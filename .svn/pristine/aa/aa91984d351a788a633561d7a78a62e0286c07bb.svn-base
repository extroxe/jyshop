app.controller("indianaUserCtrl",["$scope","_jiyin","dataToURL","$stateParams","$state",function(a,t,i,n){a.indianaList={},a.list={},a.inputPage=1,a.isAudit=!1,a.getData=function(){t.dataGet("admin/integral_indiana_admin/indiana_bet_info/"+n.id).then(function(t){a.indianaList=t.data,a.totalPage=t.total_page,angular.forEach(t.data,function(t){t.integral_indiana_result_status>0&&(a.isAudit=!0)})})},a.getData(),a.check=function(i){t.dataGet("admin/integral_indiana_admin/operate_result/"+i.result_id).then(function(i){1==i.success?(t.msg("s",i.msg),a.getData()):t.msg("e",i.msg)})},a.edit=function(n){t.dataPost("admin/integral_indiana_admin/modify_winner",i({indiana_id:n.integral_indiana_id,integral_indiana_bet_id:n.bet_id,user_id:n.user_id})).then(function(i){1==i.success?(t.msg("s",i.msg),a.getData()):t.msg("e",i.msg)})},a.nextPage=function(){a.inputPage<a.totalPage?(a.inputPage++,a.getData()):t.msg("e","当前是最后一页")},a.previousPage=function(){a.inputPage>1?(a.inputPage--,a.getData()):t.msg("e","当前是第一页")},a.firstPage=function(){a.inputPage=1,a.getData()},a.lastPage=function(){a.inputPage=a.totalPage,a.getData()},a.selectPage=function(t){a.inputPage=t,a.getData()}}]);