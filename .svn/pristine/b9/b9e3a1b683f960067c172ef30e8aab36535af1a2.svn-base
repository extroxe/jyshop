.group{
    padding: 10px;
}
.group:after, .group:before{
    height:0;
}
.nav-list{
    margin-top: 44px;
    padding: 20px 10px 10px;
}
.awards-list{text-align: center}
.awards-list img{width: 30px}
.awards-list #oDiv{
    display: inline-block;
    height: 30px;
    overflow: hidden;
    line-height: 30px;
    vertical-align: middle;
    margin-left: 10px;
    color: #e62d2d;
}

.physical-product .more{
    position: absolute;
    right: 10px;
    margin-top: -20px;
}
.awards li{
    text-align: left;
    padding: 0;
}
.grid[data-rowspace="8"] li{
    padding-top: 0;
}
.grid li > div{
    margin: 0 5px;
    padding: 10px;
    background-color: #fff;
    position: relative;
}
.grid li > div .publish-icon{
    position: absolute;
    background-color: #aaa;
    margin-top: -43%;
    right: 33%;
    border-radius: 50%;
    width: 50px;
    height: 50px;
    text-align: center;
    line-height: 50px;
    color: #fff;
    font-size: 14px;
}
.grid li > div > a > img{
    max-width: 100%;
    max-height: 100%;
}
.grid li > div > label{
    display: block;
    margin-top: 8px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    box-sizing: border-box;
    min-height: 16px;
    color: #9e9e9e;
    margin-bottom: 5px;
}
.grid li > div > span{
    font-size: 12px;
    color: #555;
}
.progress{
    padding: 1px;
}
.awards .progress-bar{
    background-color: #9dc1ab;
    width: 50%;
    height: 6px;
}
.progressbox-content{
    padding: 2px 0;
}
.progress-bar.theme-1{
    border: none;
}
.awards .publish-awards{
    display: block;
    text-align: center;
    width: 50%;
    margin: 10px auto 0;
    border: 1px solid #20aeff;
    border-radius: 10px;
}
#help-center article{
    padding: 10px;
    line-height: 25px;
}
#new-awards .lists li{
    display: block;
    background-color: #f9f9f9;
    padding: 10px 10px;
    margin-bottom: 10px;
    border-radius: 12px;
    color: #113edc;
    font-size: 18px;
    text-align: center;
}

section{
    position: fixed;
}