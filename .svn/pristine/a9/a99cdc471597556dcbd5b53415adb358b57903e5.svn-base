app.controller("drawCtrl",["$scope","_jiyin","dataToURL",function(t,a,e){t.drawList={},t.inputPage=1,t.getData=function(){a.data("",e({page:t.inputPage})).then(function(a){t.drawList=a.data,t.totalPage=a.total_page})},t.deleteData=function(n){confirm("确认删除这条数据吗?")&&a.data("",e({id:n.id})).then(function(){a.msg("s","删除成功"),t.getData()})},t.nextPage=function(){t.inputPage<t.totalPage?(t.inputPage++,t.getData()):a.msg("e","当前是最后一页")},t.previousPage=function(){t.inputPage>1?(t.inputPage--,t.getData()):a.msg("e","当前是第一页")},t.firstPage=function(){t.inputPage=1,t.getData()},t.lastPage=function(){t.inputPage=t.totalPage,t.getData()},t.selectPage=function(a){t.inputPage=a,t.getData()}}]);