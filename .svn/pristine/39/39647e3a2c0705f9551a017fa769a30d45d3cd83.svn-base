app.controller("bannerCtrl",["$scope","_jiyin","dataToURL",function(t,a,e){t.bannerList={},t.inputPage=1,t.getData=function(){a.dataGet("admin/banner_admin/get_page_info?page="+t.inputPage+"&page_size=10").then(function(a){t.bannerList=a.data,t.totalPage=a.total_page})},t.getData(),t.addBannerList=function(){t.title="增加数据",a.modal({tempUrl:"/source/admin/tpl/modal/modal-banner.html",tempCtrl:"modalBannerCtrl",ok:t.addList,size:"lg",params:{title:t.title,infoList:{},ael:"add"}})},t.addList=function(n){a.dataPost("admin/banner_admin/add",e(n)).then(function(e){1==e.success?(a.msg("s",e.msg),t.getData()):a.msg("e",e.msg)})},t.editBannerList=function(e){t.title="编辑数据",a.modal({tempUrl:"/source/admin/tpl/modal/modal-banner.html",tempCtrl:"modalBannerCtrl",ok:t.editList,size:"lg",params:{title:t.title,infoList:e,ael:"edit"}})},t.editList=function(n){a.dataPost("admin/banner_admin/update",e(n)).then(function(e){1==e.success?(a.msg("s",e.msg),t.getData()):a.msg("e",e.msg)})},t.deleteData=function(n){confirm("确认删除这条数据吗?")&&a.dataPost("admin/banner_admin/delete",e({id:n.id})).then(function(){a.msg("s","删除成功"),t.getData()})},t.nextPage=function(){t.inputPage<t.totalPage?(t.inputPage++,t.getData()):a.msg("e","当前是最后一页")},t.previousPage=function(){t.inputPage>1?(t.inputPage--,t.getData()):a.msg("e","当前是第一页")},t.firstPage=function(){t.inputPage=1,t.getData()},t.lastPage=function(){t.inputPage=t.totalPage,t.getData()},t.selectPage=function(a){t.inputPage=a,t.getData()}}]);