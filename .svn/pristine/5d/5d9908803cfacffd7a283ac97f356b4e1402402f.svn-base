*{font-family: '微软雅黑'}
#page-content .personal_list ul li{margin: 15px 0}
.form-horizontal .control-group{margin: 0;border: solid #eee; border-width: 1px 0 0; padding: 20px 10px; position: relative}
select{width:100px}
input[type *= text], input[type="password"]{box-shadow: inset 0 1px 1px rgba(0,0,0,0.0)}
.form-horizontal span{cursor: pointer; font-weight: normal}
.form-horizontal select{border-radius: 0}
.form-horizontal .model_box{margin-top: 80px;}
.form-horizontal .model_box:first-child{margin-top: 0}
.form-horizontal ._edit, .form-horizontal .edit, .form-horizontal .save_cancel, .bind, .cancel_bind{position:absolute; width:75px; right: 20px;}
.form-horizontal .prev_label{display: inline-block; width: 100px;}
.form-horizontal input.input_text{width:500px;border: none;}
.form-horizontal h5{font-family: '微软雅黑'}

#personal_info .button_img{position: absolute; right: 25px; padding: 2px 10px; margin-top: 7px;}
#_menu li a.active{color: #117d94}

#personal_info ._edit, #personal_info .edit, #account_contact ._edit, .save_cancel, .bind, .cancel_bind{text-align: right;}

#personal_info .edit-point,
#account_contact .edit-point {color: #007fff;}

.personal_list{
    border-right: 1px solid #eee;
    width: 200px;
}
.personal_list ul li .active{color: #117d94}
.personal_list ul li .active{color: #117d94}

/*收获地址*/
.table thead th, .table td{font-family: '微软雅黑';font-weight:normal;font-size: 15px; padding: 10px 4px}
.table td{font-size: 14px}
.table tbody td .fontstyle{color: #117d94}
.table tbody td .fontstyle:hover{color: red}
select, textarea, input[type="text"]{border-radius: 0}

.error { color: red; }

.pro-msg { font-size: 13px; color: #ff4c4c; }

#avatar_modal { display: none; width: 800px; left: 45%;}
#avatar_modal .modal-body { overflow-y: hidden; }
#avatar_modal #clipArea { margin: 20px; width: 60%; height: 300px; float: left; }
#avatar_modal #file,
#avatar_modal #clipBtn { margin: 20px;  }
#avatar_modal #view { margin: 0 auto; width: 26%; height: 200px; float: left; margin-left: 30px; margin-top: 70px; }

/*个人中心*/
#content{
    width: 880.69px;
    position: absolute;
    margin-left: 226px;
    border: 1px solid #ddd;
    border-top: none;
}
/*编辑个人信息*/
.model_box .control-group:first-child{
    background-color: #fff;
    width: 167px;
    text-align: center;
    position: absolute;
    margin-top: -72px;
    height: 42px;
    line-height: 42px;
    padding: 0;
    margin-left: 0px;
    border-bottom: none;
}
.model_box .control-group:nth-child(2){margin-top: 30px}
.model_box .control-group:first-child h5{
    line-height: 42px;
    font-weight: normal;
    font-size: 20px;
}

/*我的家族*/
#my_family .add_family_icon{
    position: absolute;
    margin-left: -21px;
    margin-top: 9px;
    color: #fff;
    font-size: 14px;
}
#my_family .operation_btn{
    width: 200px;
    float: right;
    text-align: right;
}
#my_family #family_info_box{margin-top: 73px}
#my_family .name_group{
    width: 780.69px;
    text-align: left;
    padding: 20px 10px;
    height: 31px;
    line-height: 28px;
}
/*添加亲属*/
.add_family{
    display: block;
    width: 800px;
    left: 173px;
    position: absolute;
    /*height: 666px;*/
}
#myModal #myModalLabel{
    padding: 10px 19px;
    font-size: 27px;
    font-family: '微软雅黑';
    font-weight: normal;
    color: #117d94;
}
#myModal .modal-body{
    max-height: 507px;
    padding: 15px 70px
}
#myModal .modal-body .control-group:first-child{
    border-top: navajowhite;
    margin-top: -5px;
}
#myModal .prev_label{
    font-family: '微软雅黑';
    font-size: 17px;
    width: 120px;
}
#myModal input.input_text{
    width: 445px;
}
.treatment{width: 443px;  display: inline-block;}
.treatment li{
    margin-right: 10px;
}
.treatment li,
.treatment li input,
.treatment li label{
    display: inline-block;
}
.treatment li input{
    margin-top: -3px;
}

#member_center .control-group img {
    width: 25px;
    height: 25px;
    vertical-align: sub;
}

#member_center table thead tr th {
    padding-bottom: 10px;
}

#member_center table tbody tr th {
    font-weight: lighter;
    border-width: 1px;
    border-style: solid none;
    border-color: #dadada;
    padding: 10px 0;
}
