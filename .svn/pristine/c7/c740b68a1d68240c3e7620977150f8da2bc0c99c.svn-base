<div ng-controller="drawUserCtrl">
    <div class="bg-light lter b-b wrapper-md">
        <h1 class="m-n font-thin h3">积分抽奖结果管理</h1>
    </div>
    <div class="wrapper-md">
        <div class="row">
            <div class="col-md-2">
                <div>
                    <a class="btn m-b-xs btn-sm btn-success btn-addon" style="margin-left: 35px;width: 140px" href="#/app/integralDraw"><i class="fa fa-angle-double-left"></i>返回积分抽奖</a>
                </div>
            </div>
        </div>
    </div>
    <div>
        <div class="wrapper-md">
            <div class="panel panel-default">
                <div class="panel-heading">
                    积分抽奖结果表
                </div>
                <div class="panel-body b-b b-light">
                <span class="pull-right">
                    Search: <input id="filter" type="text" class="form-control input-sm w-sm inline m-r"/>
                </span>
                </div>
                <div>
                    <table class="table table-striped m-b-none" ui-footable="footable" data-filter="#filter">
                        <thead>
                        <tr>
                            <th>
                                用户
                            </th>
                            <th>
                                电话号码
                            </th>
                            <th>
                                奖品名称
                            </th>
                            <th>
                                积分
                            </th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr ng-repeat="data in drawuserList">
                            <td>{{data.nickname}}</td>
                            <td>{{data.phone}}</td>
                            <td>{{data.prize_name}}</td>
                            <td>{{data.point}}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
                <nav>
                    <ul class="pager">
                        <li><a ng-click="firstPage()">第一页</a></li>
                        <li><a ng-click="previousPage()">上一页</a></li>
                        <li> 当 前 第 <input type="text" style="padding: 5px; width: 50px; text-align: center" ng-model="inputPage" ng-change="selectPage(inputPage)"> 页 / 共 {{totalPage}} 页 </li>
                        <li><a ng-click="nextPage()">下一页</a></li>
                        <li><a ng-click="lastPage()">最后一页</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </div>
</div>
