app.controller("indianaUserCtrl",["$scope","_jiyin","dataToURL","$stateParams","$state",function(a,t,n,e){a.indianaList={},a.inputPage=1,a.getData=function(){t.dataGet("admin/integral_indiana_admin/get_user_info_by_id/"+e.id).then(function(t){a.indianaList=t.data})},a.getData(),a.check=function(a){t.dataGet("admin/integral_indiana_admin/operate_result/"+a.id).then(function(a){1==a.success?t.msg("s",a.msg):t.msg("e",a.msg)})},a.nextPage=function(){a.inputPage<a.totalPage?(a.inputPage++,a.getData()):t.msg("e","当前是最后一页")},a.previousPage=function(){a.inputPage>1?(a.inputPage--,a.getData()):t.msg("e","当前是第一页")},a.firstPage=function(){a.inputPage=1,a.getData()},a.lastPage=function(){a.inputPage=a.totalPage,a.getData()},a.selectPage=function(t){a.inputPage=t,a.getData()}}]);